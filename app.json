{
  "expo": {
    "name": "TimeEgg",
    "slug": "timeegg",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "timeegg",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.timeegg.app"
    },
    "android": {
      "package": "com.timeegg.app",
      "adaptiveIcon": {
        "backgroundColor": "#0ea5e9"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
